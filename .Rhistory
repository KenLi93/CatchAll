parallel = F,
control = list(ncores = ceiling(detectCores()/2))) {
options(warn = -1)
ii <- input_data$index
input_data <- convert(input_data)
all_results_tib <- all_parametric_model(input_data, parallel = parallel, control = control) %>%
.[complete.cases(.),]
